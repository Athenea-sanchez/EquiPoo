//Impresora
public class Impresora {
    
    // Atributos
    private String modelo;
    private boolean status; // true = tiene tinta, false = sin tinta

    // Constructor
    public Impresora(String modelo) {
        this.modelo = modelo;
        this.status = true;
    }
    
    // Métodos
    public void imprimir() {
        System.out.println("Se está imprimiendo... :)");
        this.status = false; // simulamos que la tinta se gastó
    }

    // Getter
    public String getModelo() {
        return this.modelo;
    }
    
    public boolean tieneTinta() {
        return this.status;
    }
    
    public void getStatus() {
        if (this.status){
            System.out.println("Tinta suficiente :)");
        } else {
            System.out.println("Se acabó la tinta :(");
        }
    }
    
    // Setter
    public void setStatus(boolean status) {
        this.status = status;
    }
}
